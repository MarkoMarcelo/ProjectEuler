#include <iostream>
using namespace std;

int main(){
    int n,suma=0;
    n=15;
    for(int a=1; a<n;a++)
    {
        int aux=0;
        for(int div=1; div<=a;div ++)
            if(a%div==0 && a%1==0)
            aux++;
            if(aux==2)
            {
            cout<<a<<endl;
            suma=suma+a;
            }
    }
    cout<<"Suma: "<<suma<<endl;
    system("pause");
    return 0;
}

