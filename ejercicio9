#include <iostream>
using namespace std;

const int nummax= 1000; 

int main()
	{
	for (int c= 0; c <= nummax; ++c)
		for (int b= 0; b < c; ++b)
			for (int a= 0; a < b; ++a)
				{
				if ((a * a) + (b * b) == (c * c) && a + b + c == nummax)
					{
					cout << "a = " << a << " b = " << b << " c = " << c << "\n"<< "Resultado : " << a * b * c << '\n';
					return 0;
					}
				}
	return 0;
	}

	
